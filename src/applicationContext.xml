            <map>
                <entry key="UI/UX Developer" value="Nitish Kumar"></entry>
                <entry key="Java Developer" value="Rohan Shah"></entry>
                <entry key="ASP.Net Developer" value="Raj Bhasme"></entry>
                <entry key="C# Developer" value="Harshal Mandale"></entry>
            </map>
        </constructor-arg>
    </bean>
    
</beans>